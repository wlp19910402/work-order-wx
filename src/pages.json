{
    "easycom": {
        "autoscan": true,
        "custom": {
            "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
        }
    },
    "pages": [{
        "path": "pages/main/main",
        "style": {
            "navigationBarTitleText": "登录模板"
        }
    }, {
        "path": "pages/order/order",
        "style": {
            "navigationBarTitleText": "工单"
        }
    }, {
        "path": "pages/user/login/login",
        "style": {
            "navigationBarTitleText": "登录"
        }
    }, {
        "path": "pages/user/reg/reg",
        "style": {
            "navigationBarTitleText": "注册"
        }
    }, {
        "path": "pages/user/pwd/pwd",
        "style": {
            "navigationBarTitleText": "找回密码"
        }
    }, {
        "path": "pages/user/pwd/update-password",
        "style": {
            "navigationBarTitleText": "修改密码"
        }
    }, {
        "path": "pages/user/my/my",
        "style": {
            "navigationBarBackgroundColor": "#00aff2"
        }
    }, {
        "path": "pages/user/invite/invite",
        "style": {
            "navigationBarTitleText": "邀请好友"
        }
    }, {
        "path": "pages/user/invited-user/invited-user",
        "style": {
            "navigationBarTitleText": "受邀用户"
        }
    }, {
        "path": "pages/user/invite-reg/invite-reg",
        "style": {
            "navigationBarTitleText": "邀请注册页面",
            "navigationStyle": "custom"
        }
    }],
    "tabBar": {
        "color": "#7a7e83",
        "selectedColor": "#303135",
        "backgroundColor": "#ffffff",
        "list": [{
                "pagePath": "pages/main/main",
                "text": "首页",
                "iconPath": "static/img/tabBarIcon/home-default.png",
                "selectedIconPath": "static/img/tabBarIcon/home-active.png"
            },
            {
                "pagePath": "pages/order/order",
                "text": "工单",
                "iconPath": "static/img/tabBarIcon/order-default.png",
                "selectedIconPath": "static/img/tabBarIcon/order-active.png"
            },
            {
                "pagePath": "pages/user/my/my",
                "text": "我的",
                "iconPath": "static/img/tabBarIcon/my-default.png",
                "selectedIconPath": "static/img/tabBarIcon/my-active.png"
            }
        ]
    },
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarBackgroundColor": "white",
        "backgroundColor": "white"
    }
}